extends layouts/main.pug
include mixins/videoBlock

block content
    .search__header
        h3 Searching for #{searchingBy}
    .search__videos
        if video_db.length === 0
            h5 No Videos Found
        each item in video_db
            +videoBlock({
                title: item.title,
                views: item.views,
                videoFile: item.fileUrl,
                id: item.id
            })
        
